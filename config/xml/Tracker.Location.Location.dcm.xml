<!-- config/xml/Location.Location.dcm.xml -->
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Tracker\Location\Location" table="tracker">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="date" type="date" />
        <field name="time" type="time" />
        <field name="batt" type="integer" />
        <field name="smsrf" type="string" />
        <field name="loc" type="string" />
        <field name="locacc" type="integer" />
        <field name="localt" type="decimal" />
        <field name="locspd" type="decimal" />
        <field name="loctms" type="integer" />
        <field name="locn" type="string" />
        <field name="locnacc" type="string" />
        <field name="locntms" type="integer" />
        <field name="cellid" type="string" />
        <field name="cellsig" type="integer" />
        <field name="cellsrv" type="string" />
    </entity>
</doctrine-mapping>
